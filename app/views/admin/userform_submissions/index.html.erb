<h1>Menu::<%= @userform.name %>::Submissions</h1>
<div class="card">
  <div class="card">
    <div class="card-body" data-controller='menuitem'>
      <table class="table">
        <thead>
          <tr>
            <th>id</th>
            <th>Created</th>
            <th class='actions'></th>
          </tr>
        </thead>
        <tbody>
          <% @userform.userform_submissions.each do |userform_submission| %>
            <tr>
              <td><%= userform_submission.id %></td>
              <td><%= userform_submission.created_at %></td>
              <td>Delete</td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
